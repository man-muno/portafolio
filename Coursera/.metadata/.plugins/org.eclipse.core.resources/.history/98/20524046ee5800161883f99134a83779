package week4;

import java.util.*;

public class LCS3 {

    private static int lcs3(int[] a, int[] b, int[] c) {
        //Write your code here
        return Math.min(Math.min(a.length, b.length), c.length);
    }

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int an = scanner.nextInt();
        int[] a = new int[an];
        for (int i = 0; i < an; i++) {
            a[i] = scanner.nextInt();
        }
        int bn = scanner.nextInt();
        int[] b = new int[bn];
        for (int i = 0; i < bn; i++) {
            b[i] = scanner.nextInt();
        }
        int cn = scanner.nextInt();
        int[] c = new int[cn];
        for (int i = 0; i < cn; i++) {
            c[i] = scanner.nextInt();
        }
        EditDistance("editing", "distances");
        System.out.println(lcs3(a, b, c));
    }
    
    
	public static int EditDistance(String vertical, String horizontal) {
		// write your code here

		String[] verticalArray = vertical.split("");
		String[] horizontalArray = horizontal.split("");
		
		int[][] resp = new int[vertical.length()+1][horizontal.length()+1];
		
		for(int i=0;i<=vertical.length();i++){
			for(int j=0;j<=horizontal.length();j++){
				if(j==0){
					resp[i][j]=i;
				} 
				if(i==0){
					resp[i][j]=j;
				}
				if (i!=0 && j!=0){
					int insertion = resp[i][j-1];
					int deletion = resp[i-1][j];
					if(vertical.charAt(i-1) == horizontal.charAt(j-1)){
						resp[i][j] = Math.max(Math.max(insertion, deletion),resp[i-1][j-1]+1);
					} else {
						resp[i][j]= Math.max(Math.max(insertion, deletion),resp[i-1][j-1]);
					}	
				}
			}
		}
		
		return resp[vertical.length()][horizontal.length()];
	}
    
}

